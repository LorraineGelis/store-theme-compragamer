      "link#icon-twitter--tablet",
      "link#icon-youtube--tablet",
      "link#icon-facebook--tablet",
      "link#icon-instagram--tablet"
    ],
    "props": {
      "blockClass": "footer__container---tablet",
      "preserveLayoutOnMobile": true,
      "fullWidth": false,
      "colSizing": "auto"
    }
  },

