  //Icono disclousure:

  "disclosure-state-indicator": {
    "title": "disclosure producto detalle - phone",
    "props": {
      "Show": "icon#angle-down",
      "Hide": "icon#angle-up"
    }
  },

  "icon#angle-down": {
    "title": "icono disclosure--down",
    "props": {
      "id": "mpa-angle--down",
      "activeClassName": "angle-down--phone",
      "mutedClassName": "angle-down--phone"
    }
  },

  "icon#angle-up": {
    "title": "icono disclosure--up",
    "props": {
      "id": "mpa-angle--up",
      "activeClassName": "angle-down--phone",
      "mutedClassName": "angle-down--phone"
    }
  },

