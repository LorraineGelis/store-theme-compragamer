      "disclosure-trigger#first",
      "disclosure-content#first"
    ],
    "props": {
      "initialVisibility": "hidden",
      "blockClass": "contenedor__disclosurefirst--phone"
    }
  },

  "disclosure-trigger#first": {
    "title": "Contenido disclousre 1",
    "children": [
      "rich-text#titleone",
